<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!DOCTYPE configuration>
<configuration>

<!-- Target: attach to process -->
<attach></attach>

<!-- Target: console process -->
<console></console>

<!-- Target: windowed process -->
<windowed></windowed>

<!-- Tracing options -->
<interactive value="false" />
<pause       value="false" />
<restart     value="false" />
<time_limit  value="0"     />
<echo        value="false" />
<break_at></break_at>
<stalk_at></stalk_at>
<events>exception</events>
<action></action>

<!-- Debugging options -->
<autodetach value="true"  />
<hostile    value="false" />
<follow     value="true"  />

<!-- Output options -->
<verbose              value="true"  />
<ignore_python_errors value="false"  />
<duplicates           value="true"  />
<firstchance          value="false" />
<memory               value="0"     />
<database></database>
<logfile></logfile>

</configuration>
